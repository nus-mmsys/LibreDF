tmf circlepacking {
    topology {
        nodes = A,B,C,D;
	edges = e1(A,B), e2(B,C), e3(C,D);
    }
    actor A {
    	computation = IntProducer;
	step = 1;
	last = 500;
    }
    actor B {
    	computation = CirclePacking;
	number = 36;
    }
    actor C {
    	computation = DrawCircle;
	scale = 100;
    }
    actor D {
    	computation = ImageShow;
    }
}

