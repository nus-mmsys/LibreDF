    }
    actor A {
        computation = Hamilton;
        nbnodes = 6;
	cpu = 0;
    }
    actor B {
        computation = Hamilton;
        nbnodes = 6;
	cpu = 1;
    }
    actor C {
        computation = Hamilton;
        nbnodes = 6;
	cpu = 2;
    }
    actor D {
        computation = Hamilton;
        nbnodes = 6;
	cpu = 3;
    }
    actor E {
        computation = Hamilton;
        nbnodes = 6;
	cpu = 0;
    }
    actor F {
        computation = Hamilton;
        nbnodes = 6;
	cpu = 1;
    }
    parameter {
	scheduling = true;
    	logging = true;
    }
}
