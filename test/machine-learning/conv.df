df conv {
    topology {
        nodes = A,B,C;
	edges = e1(A,B), e2(B,C);
    }
    actor A {
    	computation = VideoCapture;
        file_name = pedestrian.mp4;
    }
    actor B {
    	computation = Convolution;
        kernel_size = 7;
	kernel_val = 1;
    }
    actor C {
    	computation = ImageWrite;
    }
}

