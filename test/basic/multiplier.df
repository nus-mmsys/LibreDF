df adder {
    topology {
        nodes = A,B,C;
        edges = e1(A,C), e2(B,C);
    }
    actor A {
        computation = IntProducer;
        last = 64;
    }
    actor B {
        computation = IntProducer;
        last = 64;
    }
    actor C {
	input1 = e1;
	input2 = e2;
        computation = Multiplication;
    }
}
