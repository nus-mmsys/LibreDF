df gemm {
    topology {
        nodes = A,B,C,D;
	edges = e1(A,C), e2(B,C), e3(C,D);
    }
    actor A {
    	computation = Randu;
        width = 120;
        height = 100;
    }
    actor B {
    	computation = Randu;
        width = 160;
        height = 120;
    }
    actor C {
    	computation = Gemm;
	input1 = e1;
	input2 = e2;
    }
    actor D {
    	computation = ImageWrite;
    }
}

