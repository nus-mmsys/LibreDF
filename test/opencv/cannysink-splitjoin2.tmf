tmf cannysink-splitjoin2 {
    topology {
        nodes = A,B1,B2,C1,C2,D;
	edges = e1(A,B1), e2(A,B2), e3(B1,C1), e4(B2,C2), e5(C1,D), e6(C2,D);
    }
    actor A {
    	computation = VideoCaptureSplit;
	file_name = pedestrian.mp4;
	level = 2;
    }
    actor B1 {
    	computation = CvtColor;
    }
    actor B2 {
    	computation = CvtColor;
    }
    actor C1 {
    	computation = Canny;
    }
    actor C2 {
    	computation = Canny;
    }
    actor D {
    	computation = ImageSinkJoin;
	level = 2;
    }
    parameter {
	logging = true;
    }
}
