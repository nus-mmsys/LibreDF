df imwrite {
    topology {
        nodes = A,B;
	edges = e1(A,B);
    }
    actor A {
    	computation = VideoCapture;
	file_name = pedestrian.mp4;
	host = 127.0.0.1;
    }
    actor B {
    	computation = ImageWrite;
	host = 127.0.0.1;
	input_port = 7001;
    }
    parameter {
	distributed = true;
	discovery_host = 127.0.0.1;
	discovery_port = 7000;
    }
}

