tmf cannysink-splitjoin1 {
    topology {
        nodes = A,B,C,D;
	edges = e1(A,B), e2(B,C), e3(C,D);
    }
    actor A {
    	computation = VideoCaptureSplit;
	file_name = pedestrian.mp4;
	arity = 1;
    }
    actor B {
    	computation = CvtColor;
    }
    actor C {
    	computation = Canny;
    }
    actor D {
    	computation = ImageSinkJoin;
	arity = 1;
    }
    parameter {
	logging = true;
    }
}
