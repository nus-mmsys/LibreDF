tmf canny-splitjoin1 {
    topology {
        nodes = A,S,B,C,J,D;
	edges = e0(A,S),e1(S,B), e2(B,C), e3(C,J), e4(J,D);
    }
    actor A {
    	computation = VideoCaptureMR;
	file_name = pedestrian.mp4;
    }
    actor S {
    	computation = MatSplit;
	arity = 1;
    }
    actor B {
    	computation = CvtColor;
    }
    actor C {
    	computation = Canny;
    }
    actor J {
    	computation = MatJoin;
	arity = 1;
    }
    actor D {
    	computation = ImageWriteMR;
    }
    parameter {
	logging = true;
    }
}
